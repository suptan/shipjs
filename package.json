{
  "name": "shipjs",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "migrate:sequelize": "npm run build && sequelize db:migrate",
    "clean": "rm -rf dist/*",
    "start:dev": "NODE_ENV=development nodemon server/index.js --exec babel-node --presets @babel/preset-env --watch src/",
    "dev": "NODE_ENV=development npm run migrate:sequelize && webpack --watch --config ./build/webpack.config.dev.js",
    "build": "npm run clean && babel src -D -d dist",
    "start": "NODE_ENV=production node dist",
    "lint:fix": "eslint --fix src",
    "lint": "eslint ./src",
    "unit-test": "NODE_ENV=test jest --silent --coverage --forceExit __tests__/unit",
    "unit-test:watch": "NODE_ENV=test jest __tests__/unit --watch"
  },
  "dependencies": {
    "@babel/runtime": "~7.8.3",
    "body-parser": "~1.19.0",
    "express": "~4.16.4",
    "express-validation": "~1.0.3",
    "joi": "~14.3.1",
    "pg": "~7.17.1",
    "pg-hstore": "~2.3.3",
    "sequelize": "~5.21.3",
    "sequelize-mock": "~0.10.2",
    "swagger-jsdoc": "~3.5.0",
    "swagger-ui-express": "~4.1.2",
    "url-loader": "~3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.6.0",
    "@babel/core": "~7.6.0",
    "@babel/plugin-transform-runtime": "~7.8.3",
    "@babel/preset-env": "~7.6.0",
    "babel-jest": "~24.9.0",
    "babel-loader": "~8.0.6",
    "babel-plugin-module-resolver": "3.1.2",
    "eslint": "~6.8.0",
    "eslint-loader": "~3.0.3",
    "eslint-plugin-jest": "~23.6.0",
    "jest": "~25.1.0",
    "jest-fetch-mock": "~3.0.1",
    "jest-junit": "~10.0.0",
    "nodemon-webpack-plugin": "~4.2.2",
    "sequelize-cli": "~5.5.1",
    "webpack": "~4.41.4",
    "webpack-cli": "~3.3.10",
    "webpack-node-externals": "~1.7.2"
  }
}
