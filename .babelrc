{
  "presets": [
    "@babel/env",
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }],
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "migrations": "./src/migrations",
          "models": "./src/models",
          "utils": "./src/utils",
          "repositories": "./src/repositories",
          "services": "./src/services",
          "mocks": "./__mocks__",
        }
      }
    ],
  ]
}
